<!-- PROPERTY SEARCH -->
<script type="text/x-template" id="property-search-template">
    <div class="property-search">
        <div class="row search-details">
            <location-autocomplete inline-template ref="location-autocomplete" :app="autocomplete" v-on:update-from-autocomplete="updateFromAutocomplete">
                <div class="col col-location">
                    <label for="location">Hotel, City Or Country</label>
                    <input value="" name="location" v-model="app.location" v-bind:placeholder="app.hasError ? localization.required : ''" type="text" class="form-control" v-bind:class="{'is-invalid': app.hasError && (app.location.length < app.minLocationChar)}" v-on:keyup.enter="$parent.submitSearch">
                </div>
            </location-autocomplete>

            <!-- DATEPICKERS -->
            <datepicker inline-template ref="datepicker" :app="datepicker.app" :reservation="datepicker.reservation" :localization="datepicker.localization" v-on:update-from-calendar="updateFromCalendar">
                <div class="col col-datepickers">
                    <div class="row">
                        <div class="col col-checkin">
                            <label for="checkin">Check-In</label>
                            <div class="datepicker-active-icon custom-checkin-active"></div>
                            <input class="checkin-calendar-alt form-control custom-checkin" v-model="app.checkInDate" name="checkin" type="text">
                            <div class="datepicker-wrapper checkin"><div class="checkin-calendar"></div></div>
                        </div>
                        <div class="col col-checkout">
                            <label for="checkout">Check-Out</label>
                            <div class="datepicker-active-icon custom-checkout-active"></div>
                            <input class="checkout-calendar-alt form-control custom-checkout" v-model="app.checkOutDate" name="checkout" type="text">
                            <div class="datepicker-wrapper checkout"><div class="checkout-calendar"></div></div>
                        </div>
                    </div>
                </div>
            </datepicker>

            <div class="col col-rooms">
                <label for="rooms">Rooms</label>
                <select class="form-control custom-select" name="rooms" v-model.number="totalRooms" v-on:change="changeNumberOfRooms">
                    <option v-for="n in 4">{{n}}</option>
                </select>
            </div>

            <div class="col col-adults">
                <label for="adults">Adults</label>
                <select class="form-control custom-select" name="rooms" v-model.number="rooms[0].numAdults" v-show="totalRooms==1">
                    <option v-for="n in 9">{{n}}</option>
                </select>
                <input class="form-control custom-select" v-model.number="totalAdults" readonly="true" v-show="totalRooms>1" v-on:click="showRoomList">
            </div>

            <div class="col col-children">
                <label for="children">Children</label>
                <input class="form-control custom-select" name="children" type="text" readonly="true" v-model.number="totalChildren" v-on:click="showRoomList">
            </div>

            <div class="col col-num-guests">
                <label for="children">No. of Guests</label>
                <input class="form-control custom-select" name="numguests" type="text" readonly="true" v-model.number="totalGuests" v-on:click="showRoomList">
            </div>


            <div class="col col-button">
                <a tabindex="0" class="btn btn-sm btn-block btn-primary btn-submit" v-bind:class="{'btn-active': autocomplete.hasError==false && (autocomplete.location.length >= autocomplete.minLocationChar)}" v-on:click="submitSearch()">Search</a>
            </div>
        </div>

        <transition v-on:enter="transitionEnter" v-on:leave="transitionLeave" :duration="transitionTime">
            <div class="room-list" v-show="roomListVisible">
                <hr />
                <transition-group name="roomList" v-on:enter="transitionEnter" v-on:leave="transitionLeave" :duration="transitionTime">
                    <div class="form-group" v-for="(room, rIndex) in rooms" v-bind:key="room.id">
                        <div class="row">
                            <div class="col col-room-name">
                                <label>Room: {{rIndex+1}}</label>
                            </div>
                            <div class="col col-adults-drop">
                                <label for="adultsDrop">Adults</label>
                                <select class="form-control custom-select" name="adultsDrop" v-model.number="room.numAdults">
                                    <option v-for="n in 9">{{n}}</option>
                                </select>
                            </div>
                            <div class="col col-children-drop">
                                <label for="childrenDrop">Children</label>
                                <select class="form-control custom-select" name="childrenDrop" v-model.number="room.numChildren" v-on:change="changeNumberOfChildren(room)">
                                    <option v-for="n in 11">{{n-1}}</option>
                                </select>
                            </div>
                            <div class="col col-child-ages" v-show="room.numChildren>0">
                                <label>Age Of Child</label>
                                <div class="row">
                                    <div class="col col-child-age-drop" v-for="(child, cIndex) in room.childAges">
                                        <div class="form-group">
                                            <select class="form-control custom-select" v-model.number="room.childAges[cIndex]">
                                                <option v-for="n in 17">{{n}}</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </transition-group>
                <div class="row">
                    <div class="col col-button">
                        <a class="btn btn-sm close-room-list" v-on:click="hideRoomList">Close</a>
                    </div>
                </div>
            </div>
        </transition>
        <div class="row mobile-submit">
            <div class="col">
                <a tabindex="0" class="btn btn-sm btn-block btn-primary btn-submit" v-bind:class="{'btn-active': autocomplete.hasError==false && autocomplete.location.length}" v-on:click="submitSearch">Search</a>
            </div>
        </div>
    </div>
</script>

<!-- CURRENCY CONVERTER -->
<script type="text/x-template" id="currency-converter-template">
    <div id="currencyConverter" class="currency-converter">
        <div class="dropdown">
            <button type="button"
                    id="currencyConverterDropdown"
                    data-toggle="dropdown"
                    data-flip="false"
                    aria-haspopup="true"
                    aria-expanded="false"
                    class="btn btn-dropdown"
                    aria-invalid="false">
                {{currencyDropdownText}}
            </button>
            <div aria-labelledby="currencyConverterDropdown" class="dropdown-menu">
                <div class="group" v-for="group in groupedCurrencies" :key="group.groupName">
                    <h8 class="subtitle">{{group.groupName}}</h8>
                    <button type="button" class="dropdown-item" v-for="item in group.currencies" :key="item.code"
                            v-on:click="setCurrency(item)">
                        <span>{{item.name}}</span>
                        <span>{{item.code}}</span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</script>


